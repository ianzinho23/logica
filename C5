#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <string.h>
	int main(){
		setlocale(LC_ALL, "");
		//Definindo Variáveis
		int idade;
		//Solicitando dados
		printf("Digite sua idade: ");	
		scanf("%i",&idade);
		//Verificando se é obrigado a votar
		if(idade<=18 || idade>=65){
			printf("Não é obrigado a votar.");
		}
		else{
			printf("OBRIGADO A VOTAR!");
		}
		return 0;
	}
